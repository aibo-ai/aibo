{
  "name": "technical-seo-validator",
  "version": "2.0.0",
  "description": "Technical SEO Validator Azure Function with Lighthouse integration",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "prestart": "npm run build",
