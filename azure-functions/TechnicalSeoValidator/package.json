    "lighthouse": "^11.4.0",
    "chrome-launcher": "^1.0.0",
    "puppeteer": "^21.6.1",
    "axe-core": "^4.8.3",
    "axe-puppeteer": "^1.1.0",
    "unified": "^11.0.4",
    "rehype-parse": "^9.0.0",
    "unist-util-visit": "^5.0.0",
    "uuid": "^9.0.1",
    "cheerio": "^1.0.0-rc.12",
    "node-html-parser": "^6.1.12",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@types/validator": "^13.11.7",
    "typescript": "^5.3.3",
